{
  "name": "Audio Router",
  "autoStart": true,
  "autoStartDelay": 500,
  "deviceList": {
    "AudioInput": [
      {
        "deviceType": "AudioInput",
        "name": "SSL2",
        "channels": 2,
        "sampleRate": 44100,
        "bitDepth": 16,
        "destination": "filter1",
        "device": "plughw:CARD=S2,DEV=0"
      }
    ],
    "AudioFilter": [
      {
        "name": "filter1",
        "sampleRate": 44100,
        "channels": 1,
        "bitDepth": 16,
        "destination": "Output"
      }
    ],
    "AudioOutput": [
      {
        "name": "Output",
        "sampleRate": 44100,
        "channels": 1,
        "bitDepth": 16,
        "device": "hw:CARD=Headphones,DEV=0"
      }
    ],
    "Spacer": [
      {
        "name": "Inputs",
        "displayOrder": 0,
        "displayWidth": "40px"
      },
      {
        "name": "Outputs",
        "displayOrder": 2,
        "displayWidth": "40px"
      },
      {
        "name": "Returns",
        "displayOrder": 6,
        "displayWidth": "40px"
      }
    ]
  }
}