{
  "name": "Audio Router",
  "autoStart": true,
  "autoStartDelay": 500,
  "deviceList": {
    "AudioInput": [
      {
        "deviceType": "AudioInput",
        "name": "SSL2",
        "channels": 1,
        "sampleRate": 48000,
        "bitDepth": 16,
        "destinations": [
          "VolDirect",
          "VolRtp"
        ],
        "device": "plughw:CARD=S2,DEV=0"
      }
    ],
    "AudioVolumeTransform": [
      {
        "name": "VolDirect",
        "destinations": [
          "Mixer"
        ],
        "channels": 1,
        "sampleRate": 48000,
        "bitDepth": 16,
        "mute": true,
        "displayOrder": 1,
        "soloGroup":"test"
      },
      {
        "name": "VolRtp",
        "destinations": [
          "RtpOut"
        ],
        "channels": 1,
        "sampleRate": 48000,
        "bitDepth": 16,
        "mute": false,
        "displayOrder": 1,
        "soloGroup":"test"
      }
    ],
    "RtpOpusOutput": [
      {
        "name": "RtpOut",
        "rtpIP": "127.0.0.1",
        "channels": 1,
        "sampleRate": 48000,
        "bitDepth": 16
      }
    ],
    "RtpOpusInput": [
      {
        "name": "RtpIn",
        "rtpIP": "127.0.0.1",
        "channels": 1,
        "sampleRate": 48000,
        "bitDepth": 16,
        "destinations": [
          "Mixer"
        ]
      }
    ],
    "AudioMixer": [
      {
        "name": "Mixer",
        "channels": 1,
        "sampleRate": 48000,
        "bitDepth": 16,
        "destinations": [
          "Output"
        ]
      }
    ],
    "AudioOutput": [
      {
        "name": "Output",
        "sampleRate": 48000,
        "channels": 1,
        "bitDepth": 16,
        "device": "hw:CARD=Headphones,DEV=0"
      }
    ],
    "Spacer": [
      {
        "name": "Inputs",
        "displayOrder": 0,
        "displayWidth": "40px"
      },
      {
        "name": "Outputs",
        "displayOrder": 2,
        "displayWidth": "40px"
      },
      {
        "name": "Returns",
        "displayOrder": 6,
        "displayWidth": "40px"
      }
    ]
  }
}